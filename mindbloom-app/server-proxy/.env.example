# Server Proxy Environment Variables
# Copy this file to .env and fill in your values

# Port Configuration
PORT=5001

# Google Cloud Service Account Key Path
# IMPORTANT: Never commit the actual service-account-key.json file
# Download it from Google Cloud Console and place it in this directory
GOOGLE_APPLICATION_CREDENTIALS=./service-account-key.json

# Google Cloud Project Configuration
GOOGLE_CLOUD_PROJECT_ID=your-project-id
GOOGLE_CLOUD_REGION=us-central1
GOOGLE_CLOUD_STORAGE_BUCKET=mindbloom-app-storage

# Gemini API Key (for fallback/development)
# Get this from: https://makersuite.google.com/app/apikey
GEMINI_API_KEY=your-gemini-api-key

# CORS Configuration
CORS_ORIGIN=http://localhost:3000
